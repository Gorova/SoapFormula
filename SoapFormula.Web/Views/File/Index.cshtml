@model IEnumerable<SoapFormula.Web.ViewModel.FileViewModel>
@{
    ViewBag.Title = "Index";
}

<h2>Index File</h2>
<p>@Html.ActionLink("Add new file", "Create")</p>

<table>
    <tr>
        <th></th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td></td>
            <td>
                @Html.ActionLink("Edit", "Edit", new {id = item.Id})|
                @Html.ActionLink("Delete", "Delete", new {id = item.Id})|
                @Html.ActionLink("More information","Details",new {id = item.Id})
            </td>
        </tr>
    }
</table>